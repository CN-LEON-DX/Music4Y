mixin actionDownloadAdd(custome, colorBg, colorIcon, fontSize)
    div(class="flex mt-6 gap-4 items-center justify-end ")
        button(class="w-[30px] h-[30px] border-gray-400 rounded-full border border-2 text-sm text-center")
            <i class="fa-solid fa-arrow-down" style="color: #D3D3D3;"></i>
        button(class="w-[30px] h-[30px] border-gray-400 rounded-full border border-2 text-sm text-center")
            i(class="fa-solid fa-plus" style=`color: #D3D3D3;`)
        button(class="w-[30px] h-[30px] border-gray-400 rounded-full text-sm text-center")
            <i class="fa-solid fa-ellipsis" style="color: #D3D3D3;"></i>
        button(class="w-[44px] h-[44px] rounded-full text-center", style=`background-color: #4A148C;`)
            i(class="fa-solid fa-play", style=`color: #ffffff;`)